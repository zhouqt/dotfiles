#bindkey -v

alias t='todo.sh'
alias ts='todo.sh show'
alias ta='t a'
alias tp='t p'
alias td='t do'
alias tw='t wait'
alias tc='t continue'
compdef t=todo.sh

#export GEM_HOME=$HOME/.gems/1.8

export PATH=$PATH:/sbin:/usr/sbin:/usr/games
alias pw="vim ~/.auth.bfa"

compdef ssh-copy-id=ssh

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"
source /usr/share/autojump/autojump.zsh

export RUBYLIB=$HOME/dotfiles/lib/ruby

alias vim='vim -X'
